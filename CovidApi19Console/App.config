<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true"
           indentsize="2">
      <listeners>
        <add name="text"
             type="System.Diagnostics.TextWriterTraceListener"
             initializeData="covid19apiconsole.log" />
        <add name ="console"
             type ="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>